<LinearLayout android:gravity="center_horizontal" android:layout_gravity="bottom|center_vertical|center_horizontal|center" android:id="@+id/LinearLayout02" android:paddingTop="5dp" android:layout_width="match_parent" android:layout_height="wrap_content">
