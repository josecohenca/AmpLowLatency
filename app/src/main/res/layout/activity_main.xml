        <SeekBar android:id="@+id/SeekBar04" android:paddingLeft="20dp" android:paddingRight="20dp" android:layout_width="wrap_content" android:layout_height="wrap_content" android:max="1" android:minWidth="50dp"/>
        <TextView android:id="@+id/TextView04" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="Freq Mod Off/On"/>
